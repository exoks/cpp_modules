//====< iter >==================================================================
template <typename T>
void	iter(T *arr, int len, void fun(T & element))
{
	for (int i = 0; i < len; i++)
		fun(arr[i]);
}

//====< iter >==================================================================
template <typename T>
void	iter(T *arr, int len, void fun(const T & element))
{
	for (int i = 0; i < len; i++)
		fun(arr[i]);
}
