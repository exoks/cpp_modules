//====< easyfind >==============================================================
template <typename T>
void	easyfind(T container, int x)
{
	if (find(container.begin(), container.end(), x) == container.end())
		throw (std::out_of_range("Not found"));
	std::cout << "Exist: " << x << std::endl;
}
